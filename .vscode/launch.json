    "version": "0.2.0",
    "configurations": [
        {
            "name": "chatapp_ui (localhost)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=8080",
                "--dart-define=host=localhost:8088",
            ]
        },
        {
            "name": "chatapp_ui (ip)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=8080",
                "--dart-define=host=192.168.1.2:8088",
            ]
        },
        {
            "name": "chatapp_ui (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port=8080",
                "--dart-define=host=192.168.1.2:8088",
            ]
        },
        {
            "name": "chatapp_ui (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--web-port=8080",
                "--dart-define=host=192.168.1.2:8088",
            ]
        }
    ]
}